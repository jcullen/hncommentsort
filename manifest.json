{
  "name": "HN Comment Sort",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Maps hacker news comments to a backbone collection to facilitate sorting",
  "content_scripts": [
    {
      "matches": ["*://news.ycombinator.com/item*"],
      "js": ["underscore-min.js","jquery-1.8.3.min.js","backbone-min.js","main.js"],
      "css": ["style.css"]
     }
  ]
}